{
  "workbench.colorTheme": "Qiita",
  "editor.fontSize": 14,
  "editor.rulers": [80, 120],
  "editor.tabSize": 2,
  "[python]": {
    "editor.tabSize": 4
  },
  "window.titleBarStyle": "custom",
  "workbench.colorCustomizations": {
    "menu.background": "#364549",
    "menu.foreground": "#fafafa",
    "menu.selectionBackground": "#4c6166",
    "titleBar.activeBackground": "#2c383a",
    "titleBar.border": "#4c6166",
    "menubar.selectionBackground": "#4c6166",
    "breadcrumbPicker.background": "#2c383a",
    "breadcrumb.foreground": "#fafafa",
    "editorInlayHint.foreground": "#14a4c8",
    "editorInlayHint.background": "#2c383a"
  },
  "breadcrumbs.enabled": true,
  "editor.renderWhitespace": "all",
  "workbench.iconTheme": "simple-icons",
  "editor.minimap.enabled": false,
  "editor.fontWeight": "400",
  "editor.fontFamily": "'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
